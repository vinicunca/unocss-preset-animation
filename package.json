{
  "name": "@vinicunca/unocss-preset-animation",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@9.4.0",
  "description": "UnoCSS preset animations",
  "author": "praburangki<https://github.com/praburangki>",
  "license": "MIT",
  "homepage": "https://github.com/vinicunca/unocss-preset-animation#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vinicunca/unocss-preset-animation.git"
  },
  "bugs": {
    "url": "https://github.com/vinicunca/unocss-preset-animation/issues"
  },
  "keywords": [
    "unocss",
    "unocss-preset",
    "unocss-preset-animation"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "unbuild",
    "dev": "vite playground",
    "stub": "unbuild --stub",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp package.json",
    "publish:ci": "pnpm publish --access public --no-git-checks",
    "test": "vitest",
    "test:update": "vitest --update",
    "gen:magic-animate": "esno scripts/magic-animate.ts"
  },
  "devDependencies": {
    "@types/node": "^20.14.9",
    "@types/postcss-js": "^4.0.4",
    "@vinicunca/eslint-config": "^2.6.1",
    "@vinicunca/perkakas": "^1.0.0",
    "@vitejs/plugin-vue": "^5.0.5",
    "bumpp": "^9.4.1",
    "eslint": "^9.6.0",
    "esno": "^4.7.0",
    "magic.css": "^1.4.8",
    "postcss": "^8.4.39",
    "postcss-js": "^4.0.1",
    "prettier": "^3.3.2",
    "typescript": "^5.5.2",
    "unbuild": "^2.0.0",
    "unocss": "^0.61.0",
    "vite": "^5.3.2",
    "vitest": "^1.6.0",
    "vue": "^3.4.31"
  }
}
